{
  "name": "mercurius-chat",
  "version": "0.22.0",
  "description": "Micro service chat",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "rootDir": "src",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier-eslint src/**/*.ts --write",
    "lint": "eslint --ext .ts src --fix",
